{
  "version": 2,
  "name": "Food-Code-Server",
  "builds": [{ "src": "./app/server.js", "use": "@vercel/node" }],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "./app/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://food-code-client.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}